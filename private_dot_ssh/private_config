Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa

Include ~/.colima/ssh_config
# DevPod Start vscode-remote-try-node.devpod
Host vscode-remote-try-node.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  ProxyCommand /usr/local/bin/devpod ssh --stdio --context default --user node vscode-remote-try-node
  User node
# DevPod End vscode-remote-try-node.devpod